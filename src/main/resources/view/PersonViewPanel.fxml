<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<ScrollPane style="-fx-background-color: transparent;" HBox.hgrow="ALWAYS" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
<VBox minWidth="370.0" HBox.hgrow="ALWAYS">
   <children>
      <Label id="name" fx:id="name" styleClass="cell_big_label" />

      <Label alignment="BOTTOM_CENTER" styleClass="cell_big_label" text="Personal Details" textAlignment="CENTER" />
      <Label fx:id="email" styleClass="cell_small_label" />
      <Label fx:id="phone" styleClass="cell_small_label" />
      <Label fx:id="nric" styleClass="cell_small_label" />
      <Label fx:id="address" styleClass="cell_small_label" />
         <VBox>
            <children>
            <ListView id="appointments" fx:id="appointments" />
            </children>
         </VBox>

   </children></VBox>
</ScrollPane>
