<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox prefWidth="150.4" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox>
            <children>
                <HBox>
                    <children>
                        <Label fx:id="id" styleClass="cell_big_label" text="\$id" />
                        <Label fx:id="booking" styleClass="cell_big_label" text="\$booking" />
                    </children>
                </HBox>
                <Label fx:id="nric" styleClass="cell_small_label" text="\$drNric" GridPane.columnIndex="2" />
            </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
        </VBox>
    </children>
</HBox>
